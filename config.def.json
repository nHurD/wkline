{
	"height": 26,
	"position": "top",
	"theme_uri": "http://lokaltog.github.io/wkline-default-theme/",
	"background": "#111",
	"widgets_enabled": [
		"desktops",
		"window_title",
		"now_playing_mpd",
		"volume",
		"weather",
		"external_ip",
		"battery",
		"datetime"
	],
	"widgets_config": {
		"battery": {
			"name": "BAT0"
		},
		"datetime": {
				"update_interval": 1000,
				"date_format": "DDDD-MM-YY",
				"time_format": "HH:mm:ss"
		},
		"desktops": {
				"names": ["I", "II", "III", "IV", "V"]
		},
		"external_ip": {
				"uri": "http://ipv4.icanhazip.com/"
		},
		"now_playing_mpd": {
				"update_interval": 1000,
				"host": "localhost",
				"port": 6060
		},
		"volume": {
				"card": "default",
				"selem": "Master"
		},
		"window_title": {
		},
		"weather": {
				"location": "",
				"unit": "c"
		}
	}
}
